{
    "env": {
        "es2021": true,
        "node": true,
        "mocha": true
    },
    "globals": {
        "expect": true
    },    
    "extends": "eslint:recommended",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": ["error", { "argsIgnorePattern": "next" }]
    }
}
